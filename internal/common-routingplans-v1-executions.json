{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "type": "object",
  "definitions": {
    "ExecutionRun": {
      "properties": {
        "id": {
          "type": "string"
        },
        "type": {
          "title": "ExecutionType",
          "type": "string",
          "enum": [
            "ROUTER",
            "FINALIZER"
          ],
          "tsEnumNames": [
            "ROUTER",
            "FINALIZER"
          ]
        },
        "status": {
          "title": "ExecutionStatus",
          "type": "string",
          "enum": [
            "INITIAL",
            "RUNNING",
            "FINISHED"
          ],
          "tsEnumNames": [
            "INITIAL",
            "RUNNING",
            "FINISHED"
          ]
        },
        "created": {
          "type": "Date"
        },
        "lastModified": {
          "type": "Date"
        },
        "started": {
          "type": "Date"
        },
        "finished": {
          "type": "Date"
        },
        "processedPlans" : {
          "type": "number"
        },
        "additionalinformation" : {
          "type": "object"
        }
      },
      "required": [
        "id",
        "type",
        "status",
        "created"
      ]
    }
  }
}
