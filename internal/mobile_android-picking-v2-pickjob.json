{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "title": "AndroidPickJob",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "minLength": 1
    },
    "shortId": {
      "type": "string",
      "minLength": 1
    },
    "facilityRef": {
      "type": "string",
      "minLength": 1
    },
    "shipmentRef": {
      "type": "string",
      "minLength": 1
    },
    "orderDate": {
      "type": "string",
      "minLength": 1
    },
    "targetTime": {
      "type": "string",
      "minLength": 1
    },
    "orderedArticleCount": {
      "type": "integer",
      "minimum": 1
    },
    "pickedArticleCount": {
      "type": "integer",
      "minimum": 0
    },
    "status": {
      "type": "string",
      "minLength": 1
    },
    "labelStatus": {
      "title": "AndroidLabelStatus",
      "type": "string",
      "enum": [
        "NOT_REQUESTED",
        "REQUESTED",
        "AVAILABLE",
        "REQUEST_FAILED"
      ],
      "tsEnumNames": [
        "NOT_REQUESTED",
        "REQUESTED",
        "AVAILABLE",
        "REQUEST_FAILED"
      ]
    },
    "channel": {
      "title": "AndroidShipmentChannel",
      "type": "string",
      "enum": [
        "SHIPPING",
        "COLLECT"
      ],
      "tsEnumNames": [
        "SHIPPING",
        "COLLECT"
      ]
    },
    "tenantOrderId": {
      "type": "string",
      "minLength": 1
    },
    "version": {
      "type": "integer",
      "minimum": 0
    },
    "targetVersion": {
      "type": "integer",
      "minimum": 0
    },
    "lastModificationDate": {
      "type": "string",
      "minLength": 1
    },
    "hasScannableCodes": {
      "type": "boolean"
    },
    "priority": {
      "type": "string",
      "minLength": 1
    }
  },
  "required": [
    "id",
    "shortId",
    "facilityRef",
    "channel",
    "orderDate",
    "targetTime",
    "orderedArticleCount",
    "pickedArticleCount",
    "status",
    "labelStatus",
    "version",
    "targetVersion",
    "lastModificationDate",
    "priority"
  ]
}
