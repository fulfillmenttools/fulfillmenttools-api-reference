{
  "$schema": "http://json-schema.org/draft/2019-09/schema#",
  "type": "object",
  "definitions": {
    "WebFacilityCarrierConnection": {
      "properties": {
        "carrierRef": {
          "type": "string",
          "minLength": 1
        },
        "status": {
          "type": "string",
          "enum": [
            "ACTIVE",
            "INACTIVE",
            "NOTSET"
          ]
        },
        "key" : {
          "type": "string"
        },
        "name" : {
          "type": "string"
        },
        "logoUrl": {
          "type": "string"
        },
        "deliveryType": {
          "type": "string",
          "enum": [
            "DELIVERY",
            "SAMEDAY"
          ]
        },
        "cutoffTime": {
          "properties": {
            "hour": {
              "type": "number"
            },
            "minute": {
              "type": "number"
            }
          },
          "required": [
            "hour",
            "minute"
          ]
        },
        "version": {
          "type": "integer",
          "minimum": 0
        },
        "targetVersion": {
          "type": "integer",
          "minimum": 0
        }
      },
      "type": "object",
      "required": [
        "carrierRef",
        "status",
        "key",
        "deliveryType",
        "name"
      ]
    }
  }
}
