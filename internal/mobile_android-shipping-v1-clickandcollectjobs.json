{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "title": "AndroidClickAndCollectJob",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "minLength": 1
    },
    "shortId": {
      "type": "string",
      "minLength": 1
    },
    "facilityRef": {
      "type": "string",
      "minLength": 1
    },
    "customerName": {
      "type": "string",
      "minLength": 1
    },
    "orderedArticleCount": {
      "type": "integer",
      "minimum": 0
    },
    "pickedArticleCount": {
      "type": "integer",
      "minimum": 0
    },
    "status": {
      "title": "AndroidClickAndCollectJobStatus",
      "type": "string",
      "enum": [
        "OPEN",
        "HANDED_OVER"
      ],
      "tsEnumNames": [
        "OPEN",
        "HANDED_OVER"
      ]
    },
    "orderDate": {
      "type": "string",
      "minLength": 1
    },
    "targetTime": {
      "type": "string",
      "minLength": 1
    },
    "pickUpDate": {
      "type": "string"
    },
    "version": {
      "type": "integer",
      "minimum": 0
    },
    "targetVersion": {
      "type": "integer",
      "minimum": 1
    },
    "tenantOrderId": {
      "type": "string",
      "minLength": 1
    },
    "lastModificationDate": {
      "type": "string",
      "minLength": 1
    },
    "priority": {
      "type": "string",
      "minLength": 1
    }
  },
  "required": [
    "id",
    "shortId",
    "facilityRef",
    "customerName",
    "orderedArticleCount",
    "pickedArticleCount",
    "status",
    "orderDate",
    "targetTime",
    "version",
    "targetVersion",
    "lastModificationDate",
    "priority"
  ]
}


