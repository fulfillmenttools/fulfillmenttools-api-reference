{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "title": "AndroidLoadUnit",
    "type": "object",
    "properties": {
        "pickJobRefs": {
            "type": "array",
            "items": {
                "type": "string",
                "minLength": 1
            }
        },
        "loadUnitTypeRef": {
            "type": "string",
            "minLength": 1
        },
        "loadUnitRefs": {
            "type": "array",
            "items": {
                "type": "string",
                "minLength": 1
            }
        },
        "name": {
            "type": "string",
            "minLength": 1
        },
        "description": {
            "type": "string",
            "minLength": 1
        },
        "iconUrl": {
            "type": "string",
            "minLength": 1
        },
        "priority": {
            "type": "integer"
        },
        "targetVersion": {
            "type": "integer",
            "minimum": 0
        },
        "version": {
            "type": "integer",
            "minimum": 0
        }
    },
    "required": [
        "pickJobRefs",
        "loadUnitTypeRef",
        "loadUnitRefs",
        "name",
        "targetVersion",
        "version"
    ]
}
