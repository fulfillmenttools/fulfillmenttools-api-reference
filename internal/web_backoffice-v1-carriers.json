{
  "$schema": "http://json-schema.org/draft/2019-09/schema#",
  "type": "object",
  "definitions": {
    "WebCarrier": {
      "properties": {
        "id": {
          "type": "string"
        },
        "credentials": {
          "properties": {
            "dhlAccountNumber": {
              "type": "string"
            },
            "dhlGkUsername": {
              "type": "string"
            }
          },
          "required": [
            "dhlAccountNumber",
            "dhlGkUsername"
          ]
        },
        "status": {
          "type": "string",
          "enum": [
            "ACTIVE",
            "INACTIVE"
          ]
        },
        "lifecycle": {
          "type": "string",
          "enum": [
            "GA",
            "BETA",
            "ALPHA"
          ]
        },
        "key" : {
          "type": "string"
        },
        "deliveryType": {
          "type": "string",
          "enum": [
            "DELIVERY",
            "SAMEDAY"
          ]
        },
        "version": {
          "type": "integer",
          "minimum": 0
        },
        "targetVersion": {
          "type": "integer",
          "minimum": 0
        }
      },
      "type": "object",
      "required": [
        "deliveryType",
        "status",
        "lifecycle",
        "key",
        "targetVersion",
        "version",
        "id"
      ]
    }
  }
}
