{
  "$schema": "http://json-schema.org/draft/2019-09/schema#",
  "type": "object",
  "definitions": {
    "WebCarrier": {
      "properties": {
        "order": {
          "type": "number",
          "default": 40
        },
        "carrierRef": {
          "type": "string"
        },
        "settings": {
          "properties": {
            "accountNumber": {
              "type": "string"
            },
            "isEnabled": {
              "type": "boolean",
              "default": false
            },
            "websiteId": {
              "type": "string"
            }
          },
          "required": [
            "accountNumber",
            "isEnabled",
            "websiteId"
          ]
        },
        "version": {
          "type": "integer",
          "minimum": 0
        },
        "type": {
          "type": "string"
        },
        "targetVersion": {
          "type": "integer",
          "minimum": 0
        }
      },
      "type": "object",
      "required": [
        "order",
        "settings",
        "type",
        "targetVersion",
        "version",
        "carrierRef"
      ]
    }
  }
}
