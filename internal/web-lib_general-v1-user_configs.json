{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "type": "object",
  "title": "WebUserConfig",
  "properties": {
    "userId": {
      "type": "string"
    },
    "role": {
      "properties": {
        "name": {
          "title": "WebConfigRole",
          "type": "string",
          "enum": [
            "FULFILLER",
            "SUPERVISOR",
            "ADMINISTRATOR"
          ],
          "tsEnumNames": [
            "FULFILLER",
            "SUPERVISOR",
            "ADMINISTRATOR"
          ]
        },
        "facilityIds": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "language": {
          "type": "string",
          "enum": [
            "de_DE",
            "en_US"
          ],
          "tsEnumNames": [
            "de_DE",
            "en_US"
          ]
        }
      }
    }
  },
  "required": [
    "userId",
    "role",
    "language"
  ]
}

