{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "type": "object",
  "title": "WebUserConfig",
  "properties": {
    "userId": {
      "type": "string"
    },
    "locale": {
      "type": "string",
      "enum": [
        "de_DE",
        "en_US",
        "pl_PL",
        "ru_RU"
      ],
      "tsEnumNames": [
        "de_DE",
        "en_US",
        "pl_PL",
        "ru_RU"
      ]
    },
    "role": {
      "properties": {
        "name": {
          "title": "WebConfigRole",
          "type": "string",
          "enum": [
            "FULFILLER",
            "SUPERVISOR",
            "ADMINISTRATOR"
          ],
          "tsEnumNames": [
            "FULFILLER",
            "SUPERVISOR",
            "ADMINISTRATOR"
          ]
        },
        "facilityIds": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "version" : {
      "type": "number"
    }
  },
  "required": [
    "userId",
    "role",
    "locale",
    "version"
  ]
}

